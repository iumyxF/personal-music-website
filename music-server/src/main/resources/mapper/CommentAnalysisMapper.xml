<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xs.mapper.CommentAnalysisMapper">

    <resultMap id="BaseResultMap" type="com.xs.domain.CommentAnalysis">
        <id property="id" column="id"/>
        <result property="commentId" column="comment_id"/>
        <result property="emotionType" column="emotion_type"/>
        <result property="emotionScore" column="emotion_score"/>
        <result property="topicType" column="topic_type"/>
        <result property="isSpam" column="is_spam"/>
        <result property="spamType" column="spam_type"/>
        <result property="analysisTime" column="analysis_time"/>
        <result property="modelVersion" column="model_version"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,comment_id,emotion_type,emotion_score,topic_type,is_spam,
        spam_type,analysis_time,model_version
    </sql>
</mapper>
